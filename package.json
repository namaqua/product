{
  "name": "our-products",
  "version": "0.1.0",
  "description": "Product Information Management System",
  "private": true,
  "workspaces": [
    "engines",
    "admin"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd engines && npm run start:dev",
    "dev:frontend": "cd admin && npm run dev",
    "install:all": "npm install && cd engines && npm install && cd ../admin && npm install",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd engines && npm run build",
    "build:frontend": "cd admin && npm run build",
    "test": "npm run test:backend",
    "test:backend": "cd engines && npm run test",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd engines && npm run lint",
    "lint:frontend": "cd admin && npm run lint",
    "format": "npm run format:backend && npm run format:frontend",
    "format:backend": "cd engines && npm run format",
    "format:frontend": "cd admin && npm run format",
    "format:check": "npm run format:check:backend && npm run format:check:frontend",
    "format:check:backend": "cd engines && npm run format:check",
    "format:check:frontend": "cd admin && npm run format:check"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:namaqua/product.git"
  },
  "author": "Colin Roets",
  "license": "PRIVATE",
  "homepage": "https://github.com/namaqua/product#readme",
  "dependencies": {
    "@nestjs/swagger": "^7.4.2",
    "swagger-ui-express": "^5.0.1"
  }
}
